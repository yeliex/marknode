{
    "$schema": "https://json.schemastore.org/swcrc",
    "env": {
        "targets": {
            "node": "16"
        }
    },
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true
        },
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        },
        "target": "es2021"
    },
    "module": {
        "type": "es6"
    },
    "sourceMaps": true
}
